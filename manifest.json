{
    "name": "Harch-wa-ch",
    "description": "Hide archived messages count in whatsapp web",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://*.web.whatsapp.com/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}